{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/api/user.js",
      "methods": ["GET"]
    },
    {
      "src": "/post-image",
      "dest": "/api/image.js",
      "methods": ["POST"]
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
